pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";

contract AtheneToken is ERC20 {
    constructor(
        string memory name_,
        string memory symbol_,
        address mintTo_
    ) ERC20(name_, symbol_) {
        _mint(mintTo_, 1_000_000_000 * 1e18);
    }
}